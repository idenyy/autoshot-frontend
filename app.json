{
  "expo": {
    "name": "autoshop-frontend",
    "slug": "autoshop-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-white.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b82f6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-white.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo-white.png"
    },
    "plugins": ["expo-secure-store"]
  }
}
